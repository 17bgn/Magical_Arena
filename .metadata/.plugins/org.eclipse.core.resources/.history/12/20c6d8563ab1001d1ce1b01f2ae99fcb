import java.util.*;
public class Arrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int x=s.nextInt();
		int array[]=new int[x];
		 for(int i=0;i<x;i++)
		 {
			 array[i]=s.nextInt();
		 }
		 int len=array.length;
		 System.out.println("length= "+len);
	}

}
