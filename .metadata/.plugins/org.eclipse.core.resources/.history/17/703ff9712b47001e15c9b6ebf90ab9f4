import java.util.Scanner;

public class Tests {
	public static void main(String java[]) {
		Scanner sc = new Scanner(System.in);
		System.out.println();
		System.out.println("User please enter an integer value");
		int a = sc.nextInt();
		System.out.println("User please enter an float value");
		float b = sc.nextFloat();
		System.out.println("User please enter an double value");
		double c = sc.nextDouble();
		System.out.println("User please enter an Byte value");
		byte d = sc.nextByte();
		System.out.println("User please enter an long value");
		long e = sc.nextLong();
		System.out.println("User please enter an short value");
		long f = sc.nextShort();
		System.out.println("The A Value is : "+a);
		System.out.println("The B Value is : "+b);
		System.out.println("The C Value is : "+c);
		System.out.println("The D Value is : "+d);
		System.out.println("The E Value is : "+e);
	}
}