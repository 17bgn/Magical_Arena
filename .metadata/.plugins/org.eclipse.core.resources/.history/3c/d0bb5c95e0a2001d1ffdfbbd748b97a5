import java.util.*;
public class NestedForLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		for (int i=1;i<=10;i++)
		{
			System.out.println("Table "+i);
			for (int j=1;j<=10;j++)
			{
				System.out.println(i+" x "+j+" = "+i*j);
				
			}
			System.out.println(" ");	
		}
		
	}

}
