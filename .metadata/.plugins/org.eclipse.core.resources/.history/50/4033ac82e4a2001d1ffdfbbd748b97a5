		int f=1;
		for(int n=1;n<=10;n++)
		{
			System.out.println("Factorial values of "+n);
			for (int i=1;i<=n;i++)
			{
				f=f*i;
				System.out.println(f);	
			}
			System.out.println("Factorial of = "+f);
		}
	
		
		
		
		
		
		
		
	}

}
