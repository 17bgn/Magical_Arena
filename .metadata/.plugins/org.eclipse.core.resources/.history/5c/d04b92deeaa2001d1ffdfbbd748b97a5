import java.util.*;
public class While1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		System.out.println("Factorial");
		int n=s.nextInt();
		int i=1;
		int f=1;
		while (i<=n)
		{
			f=f*i;
			
			i++;
		}
		System.out.println("Factorial of="+f);
		
		System.out.println("\n END");
	}

}
