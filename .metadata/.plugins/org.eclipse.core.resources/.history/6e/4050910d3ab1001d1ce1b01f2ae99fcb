import java.util.*;
public class Arrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		 int array[]=new int[5];
		 for(int i=1;i<5;i++)
		 {
			 array[i]=s.nextInt();
		 }
		 int len=array.length;
		 System.out.println("length= "+len);
	}

}
