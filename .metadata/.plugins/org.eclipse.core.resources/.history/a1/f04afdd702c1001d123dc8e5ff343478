class a{
	private String name;
	private String vil;
	private int age;
	
	public String getName() {
		return name;}
	public int getAge() {
		return age;}
	public String getVil() {
		return vil;}
	public void setAge(int newage) {
		age=newage;}
	public void setName(String Nname) {
		name=Nname;}
	public void setVil(String nvil) {
		vil=nvil;
	}
	
}

public class EncapEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		a a=new a();
		a.setAge(17);
		a.setName("guru");
		a.setName("Nandipadu");
		System.out.println(a.getName());
	}

}
