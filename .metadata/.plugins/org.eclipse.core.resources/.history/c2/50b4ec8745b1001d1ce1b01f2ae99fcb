import java.util.*;
public class DoubleDimesion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the doube dimesnions ");
		int a=s.nextInt();
		int b=s.nextInt();
		int dd[][]=new int [a][b];
		for (int i=0;i<a;i++)
		{
			for(int j=0;j<b;j++)
			{
				dd[i][j]=s.nextInt();
			}
		}
	}

}
