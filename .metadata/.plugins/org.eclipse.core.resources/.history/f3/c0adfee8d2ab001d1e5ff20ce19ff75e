import java.util.*;
class moe
{
	Scanner s=new Scanner (System.in);
	int power()
	{
		System.out.println("Enter pow and  a num");
		int a=s.nextInt();
		int b=s.nextInt();
		int r=1;
		for (int j=1;j<=b;j++)
			r=r*a;
		System.out.println(b+" is the power of "+a);
		return(r);
		
	}
	void power(int m,int n)
	{
		int p=m;
		int q=n;
		int s=1;
		for (int j=1;j<=q;j++)
		{
			s=s*p;
			System.out.println(q+" is the power of "+p);
			System.out.println(q+" power of "+p+" = "+s);
		}
	}
}
public class MOEx2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		moe m=new moe();
		//m.power();
		System.out.println("enter the 2 num");
		int p=s.nextInt();
		int q=s.nextInt();
		m.power(5,4);
	}

}
