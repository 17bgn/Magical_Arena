import java.util.*;
public class Sortingarray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int a=s.nextInt();
		int arr[]=new int[a];
		for(int i=0;i<a;i++)
		{
			arr[i]=s.nextInt();
		}
		System.out.println("Sorting of elements");
		int temp;
		for (int i=0;i<a;i++)
		{
			for(int j=0;j<a;j++)
			{
				if(arr[i]<arr[j])
				{
					temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
				}
			}
		}
		System.out.println("array of elemtns after sorting");
		for (int fr:arr)
		{
			System.out.print(fr+" ");
		}

	}

}
